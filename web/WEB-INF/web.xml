<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>crearEmpresa</servlet-name>
        <servlet-class>MUsuarios.Servlets.crearEmpresa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>iniciarSesion</servlet-name>
        <servlet-class>MUsuarios.Servlets.iniciarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>MUsuarios.Servlets.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadFile</servlet-name>
        <servlet-class>MDocumentos.Servlets.uploadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crearEquipoTrabajo</servlet-name>
        <servlet-class>MDistribucion.Servlets.crearEquipoTrabajo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>downloadFile</servlet-name>
        <servlet-class>MDocumentos.Servlets.downloadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateFile</servlet-name>
        <servlet-class>MDocumentos.Servlets.updateFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteFile</servlet-name>
        <servlet-class>MDocumentos.Servlets.deleteFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addEmpleadosEquipo</servlet-name>
        <servlet-class>MDistribucion.Servlets.addEmpleadosEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarEquipo</servlet-name>
        <servlet-class>MDistribucion.Servlets.eliminarEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sacarDelEquipo</servlet-name>
        <servlet-class>MDistribucion.Servlets.sacarDelEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IntroducirDoc</servlet-name>
        <servlet-class>MDocumentos.Servlets.IntroducirDoc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>crearEmpresa</servlet-name>
        <url-pattern>/crearEmpresa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>iniciarSesion</servlet-name>
        <url-pattern>/iniciarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadFile</servlet-name>
        <url-pattern>/uploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crearEquipoTrabajo</servlet-name>
        <url-pattern>/crearEquipoTrabajo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>downloadFile</servlet-name>
        <url-pattern>/downloadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateFile</servlet-name>
        <url-pattern>/updateFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteFile</servlet-name>
        <url-pattern>/deleteFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addEmpleadosEquipo</servlet-name>
        <url-pattern>/addEmpleadosEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarEquipo</servlet-name>
        <url-pattern>/eliminarEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sacarDelEquipo</servlet-name>
        <url-pattern>/sacarDelEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IntroducirDoc</servlet-name>
        <url-pattern>/IntroducirDoc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
